# AWS Configuration
# Copy this file to .env and fill in your actual values
# The .env file is gitignored for security

# AWS Credentials (optional - can also use ~/.aws/credentials or IAM roles)
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key

# AWS Region
AWS_DEFAULT_REGION=us-east-1

# S3 Bucket Configuration (used by Transcribe and Rekognition)
S3_BUCKET=your-bucket-name

# Amazon Transcribe Configuration (for Transcribe examples)
TRANSCRIBE_S3_KEY=input.wav

# Amazon Rekognition Configuration (for Rekognition examples)
REKOGNITION_S3_KEY=input.jpg
